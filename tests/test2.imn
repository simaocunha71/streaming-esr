node n3 {
    type router
    model router
    network-config {
	hostname R2
	!
	interface eth1
	 ip address 10.0.4.1/24
	 ipv6 address 2001:4::1/64
	!
	interface eth0
	 ip address 10.0.1.2/24
	 ipv6 address 2001:1::2/64
	!
    }
    canvas c1
    iconcoords {358.0 182.0}
    labelcoords {358.0 214.0}
    interface-peer {eth0 n5}
    interface-peer {eth1 n1}
}

